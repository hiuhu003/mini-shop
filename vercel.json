{
  "version": 2,
  "builds": [
    {
      "src": "api/lambda.php",
      "use": "vercel-php@0.7.4"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/lambda.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "APP_URL": "https://mini-shop-y9rd.vercel.app/"
  }
}
